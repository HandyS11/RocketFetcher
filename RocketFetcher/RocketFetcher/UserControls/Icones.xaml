<UserControl x:Class="RocketFetcher.UserControls.Icones"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RocketFetcher.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="110" d:DesignWidth="110">

    <StackPanel Width="90" Height="90" Margin="5">
        <Button Style="{StaticResource noBC}"
                Click="Button_Click">
            <Button.Content>
                <Image Source="{Binding path}"/>
            </Button.Content>
        </Button>
    </StackPanel>
</UserControl>
